    <!-- https://www.inf.ufrgs.br/bdi/ontologies/GEORES_0000033 -->

    <owl:Class rdf:about="https://www.inf.ufrgs.br/bdi/ontologies/GEORES_0000033">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000008"/>
